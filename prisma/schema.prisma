generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Admin {
  id    String @id @default(cuid())
  pass1 String
  pass2 String
  pass3 String
  pass4 String
}
